<div class="container  bootstrap snippets bootdeys">
<div class="row">
  <div class="col-md-8 col-md-offset-2">
	  	<div class="panel panel-default invoice" id="invoice">
		  <div class="panel-body">
        <div class="row">

            <h1 class="generated-text">Edit Quote
            </h1>

            <hr>
            <br>
          </div>
          <div class="col-md-8 col-md-offset-2">




<%= form_with(model: qetquote) do |form| %>
  <% if qetquote.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(qetquote.errors.count, "error") %> prohibited this qetquote from being saved:</h2>

      <ul>
        <% qetquote.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :zip2 %>
    <%= form.text_field :zip2, class: "form-control"  %>
  </div>




  <div class="field">
    <%= form.label :firstname %>
    <%= form.text_field :firstname, class: "form-control"  %>
  </div>

  <div class="field">
    <%= form.label :lastname %>
    <%= form.text_field :lastname, class: "form-control"   %>
  </div>



  <div class="actions">

    <%= form.submit "Update", class: "btn btn-primary" %>
  </div>
  <br>
  <%= link_to 'Show', @qetquote %> |
  <%= link_to 'Back', qetquotes_path %>
<% end %>
<br>  <br>
</div>
